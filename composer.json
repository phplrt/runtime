{
    "name": "phplrt/runtime",
    "type": "metapackage",
    "description": "A set of phplrt runtime libraries",
    "license": "MIT",
    "keywords": ["runtime"],
    "homepage": "https://github.com/phplrt",
    "support": {
        "issues": "https://github.com/phplrt/runtime/issues",
        "source": "https://github.com/phplrt/runtime"
    },
    "authors": [
        {
            "name": "Kirill Nesmeyanov",
            "email": "nesk@xakep.ru"
        }
    ],
    "require": {
        "php": ">=7.4",
        "phplrt/lexer": "^3.0",
        "phplrt/parser": "^3.0",
        "phplrt/position": "^3.0",
        "phplrt/visitor": "^3.0"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.0.x-dev"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
